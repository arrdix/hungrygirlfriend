{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-use-before-define": "off",
    "no-alert": "off",
    "no-plusplus": "off",
    "no-unused-vars": "warn"
  }
}
